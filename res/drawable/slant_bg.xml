<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <rotate
            android:fromDegrees="8"
            android:toDegrees="20"
            android:pivotX="0%"
            android:pivotY="0%" >
            <shape
                android:shape="rectangle"
                android:top="1dip" >
                <solid
                    android:width="1dip"
                    android:color="#fff" />
            </shape>
        </rotate>
    </item>

</layer-list>